{
  "name": "prototypr",
  "version": "0.1.0",
  "description": "Prototyping Boilerplate",
  "main": "Gulpfile.js",
  "private": true,
  "scripts": {
    "start": "npm run build:watch",
    "test": "./node_modules/.bin/jest -u ./src/js/**/*.spec.js",
    "test:watch": "./node_modules/.bin/jest --watchAll -u ./src/js/**/*.spec.js",
    "build": "./node_modules/.bin/gulp",
    "build:css": "./node_modules/.bin/gulp css",
    "build:html": "./node_modules/.bin/gulp html",
    "build:js": "./node_modules/.bin/gulp js",
    "build:assets": "./node_modules/.bin/gulp assets",
    "build:watch": "./node_modules/.bin/gulp dev",
    "lint": "./node_modules/.bin/eslint ./src/**/*.js Gulpfile.js --fix --ignore-pattern 'wp-worker.js' --quiet",
    "deploy": "NODE_ENV=production npm run build && firebase deploy --project=phaseout-ltd --only hosting:ghosthunter"
  },
  "keywords": [],
  "author": "Pongstr Ordillo <talk@pongstr.io>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "dayjs": "^1.8.16",
    "del": "^5.1.0",
    "eslint": "^6.5.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "exports-loader": "^0.7.0",
    "gulp": "^4.0.2",
    "gulp-data": "^1.3.1",
    "gulp-eslint": "^6.0.0",
    "gulp-header-comment": "^0.6.1",
    "gulp-nunjucks": "^5.0.1",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "jest": "^24.9.0",
    "lodash": "^4.17.15",
    "node-fetch": "^2.6.1",
    "node-sass": "^4.13.1",
    "webpack": "^4.41.2",
    "webpack-stream": "^5.2.1"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "jquery": "^3.4.1",
    "popper.js": "^1.16.0"
  }
}
